{
  "name": "react-riotapi",
  "version": "1.0.0",
  "description": "Simple react app utilizing react, redux and sagas with some additional sugar on top",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "WDS": "webpack-dev-server --inline --hot",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuckyWasTaken/react-riotapi.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuckyWasTaken/react-riotapi/issues"
  },
  "homepage": "https://github.com/LuckyWasTaken/react-riotapi#readme",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "webpack": "^4.29.5",
    "webpack-dev-server": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.10",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  }
}
